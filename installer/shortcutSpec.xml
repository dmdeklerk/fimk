<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="FIMK" location="startMenu"/>
	<shortcut
		name="FIMK Server"
		target="$INSTALL_PATH\fimk.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start FIMK server"
		iconFile="$INSTALL_PATH\html\fimk\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
	<shortcut
		name="Lompsa Wallet"
		target="http://localhost:7886/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch FIMK wallet in browser"
		iconFile="$INSTALL_PATH\html\fimk\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>